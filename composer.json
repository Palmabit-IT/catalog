    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "baum/baum": "1.0.*",
        "way/form": "dev-master",
        "intervention/image": "1.6.*",
        "palmabit/library" : "1.0.*",
        "palmabit/multilanguage" : "1.0.*",
        "palmabit/authentication" : "1.0.*",
        "jacopo/laravel-single-table-inheritance": "1.0.*@dev",
        "jacopo/bootstrap-3-table-generator": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "0.9.0",
        "orchestra/testbench": "2.2.*",
        "fzaninotto/faker": "1.4.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Palmabit\\Catalog\\": "src/"
        },
        "psr-4": {
            "Palmabit\\Catalog\\Tests\\" : "tests/"
        },
        "files" : [
            "src/Palmabit/Catalog/Helpers/helpers.php"
        ],
        "classmap" : [
            "src/controllers",
            "src/commands"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}